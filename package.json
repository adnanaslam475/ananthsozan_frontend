{
  "name": "task-manager",
  "version": "1.0.0",
  "description": "task manager api user login/logout",
  "main": "index.js",
  "engines": {
    "node": "14.x"
  },
  "scripts": {
    "start": "nodemon --experimental-modules --es-module-specifier-resolution=node server.js",
    "client-install": "npm install --prefix client",
    "client": "npm start --prefix client",
    "server": "nodemon --experimental-modules --es-module-specifier-resolution=node server.js",
    "dev": "concurrently \"npm run server\" \"npm run client\"",
    "heroku-postbuild": "NPM_CONFIG_PRODUCTION=false npm install --prefix client && npm run build --prefix client",
    "postinstall": "cd client && npm install"
  },
  "jest": {
    "testEnvironment": "node"
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "dependencies": {
    "bcryptjs": "^2.4.3",
    "cool-ascii-faces": "^1.3.4",
    "cors": "^2.8.5",
    "dotenv": "^16.0.0",
    "express": "^4.18.1",
    "fs": "0.0.1-security",
    "jsonwebtoken": "^8.5.1",
    "mongodb": "^4.5.0",
    "mongoose": "^6.3.2",
    "validator": "^13.7.0"
  },
  "devDependencies": {
    "env-cmd": "^10.0.1",
    "jest": "^25.1.0",
    "node-sass": "^7.0.1",
    "nodemon": "^2.0.15",
    "sass-loader": "^12.6.0"
  }
}
